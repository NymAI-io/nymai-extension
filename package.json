{
  "name": "nymai",
  "displayName": "Nymai",
  "version": "0.0.1",
  "description": "Clarity in every click. NymAI is an AI-powered engine that instantly verifies the authenticity and credibility of any text, image, or video.",
  "author": "Plasmo Corp. <foss@plasmo.com>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build && node scripts/secure-manifest.js",
    "package": "plasmo package"
  },
  "dependencies": {
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.78.0",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.4.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "postcss": "8.4.33",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://rpnprnyoylifxxstdxzg.supabase.co/*",
      "https://nymai-backend.onrender.com/*",
      "https://nymai-image-scraper.onrender.com/*"
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self';"
    },
    "permissions": [
      "storage",
      "contextMenus",
      "activeTab",
      "scripting",
      "tabs"
    ],
    "externally_connectable": {
      "matches": [
        "https://www.nymai.io/*",
        "https://nymai.io/*",
        "http://localhost:*/*"
      ]
    },
    "web_accessible_resources": [
      {
        "resources": [
          "NymAI_full_logo.svg",
          "NymAI_icon.svg",
          "assets/NymAI_full_logo.svg",
          "assets/NymAI_icon.svg"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ]
  }
}